class Bus implements Vehicle{

	public String type="B3 vehicle";
	@Override
	public void start() {
		// TODO Auto-generated method stub
		System.out.println("starts : "+type);
	}

	@Override
	public void move() {
		// TODO Auto-generated method stub

		System.out.println("move");
		
	}

	@Override
	public void horn() {
		// TODO Auto-generated method stub
		System.out.println("horn");
	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		System.out.println("break");
	}

	@Override
	public void gear() {
		// TODO Auto-generated method stub
		System.out.print(1);;
	}

	
	
}
